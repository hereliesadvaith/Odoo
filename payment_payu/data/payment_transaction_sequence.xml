<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--  Sequence number for payment transaction  -->
        <record id="warranty_sequence" model="ir.sequence">
            <field name="name">Payment Transaction Sequence</field>
            <field name="code">payment.transaction</field>
            <field name="prefix">TXNPU%(y)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>